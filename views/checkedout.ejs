<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Checked Out Records</title>
    <link rel="stylesheet" href="/stylesheets/checkedout.css">
</head>
<body>
    <div class="container">
        <h1>Checked Out Records</h1>
        <table class="checkout-table">
            <thead>
                <tr>
                    <th>Student ID</th>
                    <th>Student Name</th>
                    <th>Checked Out Item</th>
                </tr>
            </thead>
            <tbody id="checkout-records">
                <% checkoutRecords.forEach(record => { %>
                    <tr>
                        <td><%= record.student_id %></td>
                        <td><%= record.student_name %></td>
                        <td><%= record.item_name %></td>
                    </tr>
                <% }); %>
            </tbody>
        </table>
    </div>
</body>
</html>
