<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= title %></title>
    <link rel="stylesheet" href="/stylesheets/availableitems.css">
</head>
<body>
    <h1>Available Items</h1>
    <table>
        <thead>
            <tr>
                <th>Item ID</th>
                <th>Item Name</th>
                <th>Category</th>
                <th>Barcode</th>
                <th>Quantity</th>
            </tr>
        </thead>
        <tbody>
            <% inventoryItems.forEach(item => { %>
                <tr>
                    <td><%= item.item_id %></td>
                    <td><%= item.item_name %></td>
                    <td><%= item.item_category %></td>
                    <td><%= item.item_barcode %></td>
                    <td><%= item.item_quantity %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>
